<h1>New Artist Form</h1>
<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <p><%= e %></p>
    <% end %>
<% end %>

<%= form_for @artist do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :title %>
    <%= f.text_field :title %><br><br>

    <%= f.label :age %>
    <%= f.number_field :age %><br><br>

    <%= f.submit %>

<% end %>